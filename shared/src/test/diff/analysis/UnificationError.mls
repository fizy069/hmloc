:ns
:unify
def s x = if x then x else 3
//│ s: 'a -> 'b
//│   where
//│     'a <: 'c & bool
//│     'c :> 3
//│        <: 'b
//│     'b :> 3
//│  = [Function: s]

:ns
:unify
rec def clone x n =
  let rec helper acc n' =
    if acc <= 0 then acc else helper (acc + 1) (n' - 1) in
  helper x (n - 1)
//│ clone: 'clone
//│   where
//│     'clone :> 'a -> 'b -> 'c
//│     'b <: int
//│     'a <: 'd
//│     'd :> int
//│        <: int & 'e & number
//│     'e :> int
//│        <: 'f
//│     'f :> int
//│        <: 'c & 'g
//│     'g :> int
//│        <: 'e
//│     'c :> int
//│      = [Function: clone]

